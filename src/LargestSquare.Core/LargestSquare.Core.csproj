<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <PackageProjectUrl>https://github.com/bertt/LargestSquare</PackageProjectUrl>
    <RepositoryUrl>https://github.com/bertt/LargestSquare</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageId>LargestSquareCalculator</PackageId>
    <Authors>Bert Temme</Authors>
    <Company>Bert  Temme</Company>
    <Product>LargestSquareCalculator</Product>
    <Description>Calculates the largest square of set of tiles</Description>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="tilebelt" Version="1.3.4" />
  </ItemGroup>

</Project>
